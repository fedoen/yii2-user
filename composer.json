{
    "name": "fedoen/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2 clone from fedoen/yii2-user",
    "keywords": ["yii2", "yii2-user", "user management", "fedoen"],
    "type": "yii2-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "^2.0.13",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.1.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "fedoen\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "fedoen\\user\\Bootstrap",
        "branch-alias": {
            "dev-master": "0.9.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
